    <Style x:Key="VerticalSeparator" TargetType="{x:Type Separator}" BasedOn="{StaticResource {x:Static MenuItem.SeparatorStyleKey}}">
        <Setter Property="Background" Value="{StaticResource GreenLightest}"/>
